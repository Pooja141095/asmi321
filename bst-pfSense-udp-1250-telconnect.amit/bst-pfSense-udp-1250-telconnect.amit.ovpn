dev tun
persist-tun
persist-key
data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305:AES-256-CBC
data-ciphers-fallback AES-256-CBC
auth SHA1
tls-client
client
resolv-retry infinite
remote 74.201.101.132 1250 udp
verify-x509-name "telconnect.verscom" name
auth-user-pass
pkcs12 bst-pfSense-udp-1250-telconnect.amit.p12
tls-auth bst-pfSense-udp-1250-telconnect.amit-tls.key 1
remote-cert-tls server
explicit-exit-notify

